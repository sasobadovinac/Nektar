<?xml version="1.0" encoding="utf-8" ?>
<NEKTAR>
    <EXPANSIONS>
        <E COMPOSITE="C[7]" NUMMODES="6" TYPE="MODIFIED" FIELDS="u,v,theta,p" />
    </EXPANSIONS>

    <CONDITIONS>

        <SOLVERINFO>
            <I PROPERTY="SolverType"                    VALUE="VelocityCorrectionScheme"/>
            <I PROPERTY="EQTYPE"                        VALUE="UnsteadyNavierStokes"    />
            <I PROPERTY="EvolutionOperator"             VALUE="Adjoint"               />
            <I PROPERTY="Projection"                    VALUE="Galerkin"                />
            <I PROPERTY="TimeIntegrationMethod"         VALUE="IMEXOrder3"              />
            <I PROPERTY="Driver"                        VALUE="Arpack"                />
            <I PROPERTY="ArpackProblemType"             VALUE="LargestMag"                />
        </SOLVERINFO>

        <PARAMETERS>
            <!-- Need to pick Delta t strategically, dont want CFL number to be too large! -->
            <P> TimeStep       = 0.01                </P>

            <!--  Final time -->
            <P> TFinal         = 0.1            </P>
            <P> NumSteps       = TFinal/TimeStep          </P>
            <P> IO_CheckSteps  = 1/TimeStep          </P>
            <P> IO_InfoSteps   = 1/TimeStep                   </P>
            <P> IO_CFLSteps    = 1/TimeStep	             </P>

            <!-- Quasi-3D Params -->
            <!--P> HomModesz      = 16      </P>
            <P> LZ             = PI     </P-->

            <!-- Reynolds number -->
            <P> Pr             = 0.71               </P>
            <P> Kinvis         = Pr               </P>

            <!-- Heating parameters  -->
            <!-- Ra(Gage) = Ra(Mhm) * 8 -->
            <!-- Ra(Gage)_stable < 1708, Ra(Mhm)_stable = 213.5-->
            <P> Ra_p          = 3900              </P>
            <P> alpha         = 4               </P>

            <!-- SFD -->
            <!--<P> ControlCoeff = 1        </P>
            <P> FilterWidth  = 0.25        </P>
            <P> TOL          = 1e-8    </P>-->

            <!-- Stability analysis -->
            <P> kdim         = 16       </P>
            <P> nvec         = 1        </P>
            <P> evtol        = 1e-6     </P>
        </PARAMETERS>

        <VARIABLES>
            <V ID="0">u</V>
            <V ID="1">v</V>
            <V ID="2">theta</V>
            <V ID="3">p</V>
        </VARIABLES>

        <BOUNDARYREGIONS>
            <B ID="0"> C[1]   </B>   <!-- Inlet    -->
            <B ID="1"> C[2]   </B>   <!-- Outlet    -->
            <B ID="2"> C[3]   </B>   <!-- UWall    -->
            <B ID="3"> C[4]  </B>   <!-- Lwall    -->
        </BOUNDARYREGIONS>

        <BOUNDARYCONDITIONS>
            <REGION REF="0">
                <P VAR="u"  VALUE="[1]" />
                <P VAR="v" VALUE="[1]" />
                <P VAR="theta" VALUE="[1]" />
                <P VAR="p" VALUE="[1]" />
            </REGION>
            <REGION REF="1">
                <P VAR="u" VALUE="[0]" />
                <P VAR="v" VALUE="[0]" />
                <P VAR="theta" VALUE="[0]" />
                <P VAR="p" VALUE="[0]" />
            </REGION>
            <REGION REF="2">
                <D VAR="u" VALUE="0" />
                <D VAR="v" VALUE="0" />
                <D VAR="theta" VALUE="0" />
                <N VAR="p" VALUE="0" USERDEFINEDTYPE="H" />
            </REGION>
            <REGION REF="3">
                <D VAR="u" VALUE="0" />
                <D VAR="v" VALUE="0" />
                <D VAR="theta" VALUE ="0" />
                <N VAR="p" VALUE="0" USERDEFINEDTYPE="H" />
            </REGION>

        </BOUNDARYCONDITIONS>
        <FUNCTION NAME="DiffusionCoefficient" >
            <E VAR="theta" VALUE="1" />
        </FUNCTION>

        <FUNCTION NAME="BodyForce">
            <E VAR="u"     VALUE="0"                 EVARS="u v theta" />
            <E VAR="theta"     VALUE="v * Ra_p * Pr" EVARS="u v theta" />
            <E VAR="v" VALUE="0"                 EVARS="u v theta"/>
        </FUNCTION>

        <FUNCTION NAME="BaseFlow">
            <F FILE="ThermStab_adj_Ar.bse" />
        </FUNCTION>
        <FUNCTION NAME="InitialConditions">
            <F FILE="ThermStab_adj_Ar.rst" />
        </FUNCTION>
        <!--
            <FUNCTION NAME="ExactSolution">
                <E VAR="v"    VALUE="0" />
                <E VAR="p"    VALUE="Ra_p / 2 / Pr / Re / Re * (y - y*y/2)" />
                <E VAR="u"      VALUE="1-y^2" />
                <E VAR="theta" VALUE ="1/2 * (1.0 - y)" />
            </FUNCTION>
        -->
    </CONDITIONS>

    <FORCING>
        <FORCE TYPE="Body">
            <BODYFORCE>BodyForce</BODYFORCE>
        </FORCE>
    </FORCING>

    <FILTERS>
        <FILTER TYPE="ModalEnergy">
            <PARAM NAME="OutputFile">EnergyFile</PARAM>
            <PARAM NAME="OutputFrequency">1</PARAM>
        </FILTER>
    </FILTERS>
    <GEOMETRY DIM="2" SPACE="2">
        <VERTEX COMPRESSED="B64Z-LittleEndian" BITSIZE="64"></VERTEX>
        <EDGE COMPRESSED="B64Z-LittleEndian" BITSIZE="64"></EDGE>
        <ELEMENT>
            <Q COMPRESSED="B64Z-LittleEndian" BITSIZE="64"></Q>
        </ELEMENT>
        <COMPOSITE>
            <C ID="1"> E[3,18,29,40,51,62,73,84,95,106] </C>
            <C ID="2"> E[219,209,199,189,179,169,159,149,139,128] </C>
            <C ID="3"> E[105,108,110,112,114,117,120,123,126,129] </C>
            <C ID="4"> E[13,10,7,4,0,218,216,214,212,210] </C>
            <C ID="5"> E[113,102,91,80,69,58,47,36,25,14] </C>
            <C ID="7"> Q[0-99] </C>
        </COMPOSITE>
        <DOMAIN> C[7] </DOMAIN>
    </GEOMETRY>
