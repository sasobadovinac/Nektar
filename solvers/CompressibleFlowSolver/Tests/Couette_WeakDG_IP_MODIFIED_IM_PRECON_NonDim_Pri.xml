        <T ID="7">7 11 12</T>
        <T ID="9">10 15 14</T>
        <T ID="11">11 16 17</T>
        <T ID="13">14 20 19</T>
        <T ID="15">17 21 22</T>
        <Q ID="0">0 1 2 3</Q>
        <Q ID="2">1 6 8 5</Q>
        <Q ID="4">3 7 8 4</Q>
        <Q ID="6">8 11 13 10</Q>
        <Q ID="8">3 12 13 9</Q>
        <Q ID="10">8 16 18 15</Q>
        <Q ID="12">13 17 18 14</Q>
