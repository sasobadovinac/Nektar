<?xml version="1.0" encoding="utf-8"?>

<NEKTAR xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.nektar.info/schema/nektar.xsd">

    <GEOMETRY DIM="2" SPACE="2">

        <VERTEX>
            <V ID="0">  0.0   -5.0    0.0 </V>
            <V ID="1">  5.0   -5.0    0.0 </V>
            <V ID="2"> 10.0   -5.0    0.0 </V>
            <V ID="3">  0.0    0.0    0.0 </V>
            <V ID="4">  5.0    0.0    0.0 </V>
            <V ID="5"> 10.0    0.0    0.0 </V>
            <V ID="6">  0.0    5.0    0.0 </V>
            <V ID="7">  5.0    5.0    0.0 </V>
            <V ID="8"> 10.0    5.0    0.0 </V>
        </VERTEX>

        <EDGE>
            <E ID="0"> 0 1 </E>
            <E ID="1"> 1 2 </E>
            <E ID="2"> 0 3 </E>
            <E ID="3"> 1 4 </E>
            <E ID="4"> 2 5 </E>
            <E ID="5"> 3 4 </E>
            <E ID="6"> 4 5 </E>
            <E ID="7"> 6 3 </E>
            <E ID="8"> 4 7 </E>
            <E ID="9"> 5 8 </E>
            <E ID="10"> 6 7 </E>
            <E ID="11"> 7 8 </E>
        </EDGE>

        <ELEMENT>
            <Q ID="0"> 0 3 5 2 </Q>
            <Q ID="1"> 1 4 6 3 </Q>
            <Q ID="2"> 5 8 10 7 </Q>
            <Q ID="3"> 6 9 11 8 </Q>
        </ELEMENT>

        <COMPOSITE>
            <C ID="0"> Q[0-3]   </C>
            <C ID="1"> E[0,1]   </C>
            <C ID="2"> E[4,9]   </C>
            <C ID="3"> E[10,11] </C>
            <C ID="4"> E[2,7]   </C>
        </COMPOSITE>

        <DOMAIN> C[0] </DOMAIN>

    </GEOMETRY>

    <EXPANSIONS>
        <E COMPOSITE="C[0]"
           BASISTYPE="GLL_Lagrange,GLL_Lagrange" NUMMODES="18,18"
           POINTSTYPE="GaussLobattoLegendre,GaussLobattoLegendre" NUMPOINTS="24,24"/>
    </EXPANSIONS>

    <CONDITIONS>

        <PARAMETERS>
            <!--P> TimeStep       = 0.0284                </P-->
            <!--P> TimeStep       = 0.0193                </P-->
            <P> TimeStep       = 0                </P>
            <P> FinTime        = 100.0            </P>
            <P> NumSteps       = 0                </P>
            <P> IO_CheckSteps  = 100              </P>
            <P> IO_InfoSteps   = 1                </P>
            <P> dxForce        = 0.0672           </P>
            <P> alpha          = 1.5              </P>
            <P> Gamma          = 1.4              </P>
            <P> CFL            = 1.0              </P>
        </PARAMETERS>

        <SOLVERINFO>
            <I PROPERTY="EQType"                VALUE="IsentropicVortex"     />
            <I PROPERTY="Projection"            VALUE="DisContinuous"        />
            <I PROPERTY="TimeIntegrationMethod" VALUE="ClassicalRungeKutta4" />
            <I PROPERTY="UpwindType"            VALUE="HLLC"        />
            <I PROPERTY="ProblemType"           VALUE="IsentropicVortex"     />
        </SOLVERINFO>

        <VARIABLES>
            <V ID="0"> rho  </V>
            <V ID="1"> rhou </V>
            <V ID="2"> rhov </V>
            <V ID="3"> E    </V>
        </VARIABLES>

        <BOUNDARYREGIONS>
            <B ID="0"> C[1] </B>
            <B ID="1"> C[2] </B>
            <B ID="2"> C[3] </B>
            <B ID="3"> C[4] </B>
        </BOUNDARYREGIONS>

        <!--BOUNDARYCONDITIONS>
            <REGION REF="0">
                <D VAR="rho"  USERDEFINEDTYPE="IsentropicVortex" VALUE="0" />
                <D VAR="rhou" USERDEFINEDTYPE="IsentropicVortex" VALUE="0" />
                <D VAR="rhov" USERDEFINEDTYPE="IsentropicVortex" VALUE="0" />
                <D VAR="E"    USERDEFINEDTYPE="IsentropicVortex" VALUE="0" />
            </REGION>
        </BOUNDARYCONDITIONS-->

        <BOUNDARYCONDITIONS>
            <REGION REF="0">
                <P VAR="rho"       VALUE="[2]"/>
                <P VAR="rhou"      VALUE="[2]"/>
                <P VAR="rhov"      VALUE="[2]"/>
                <P VAR="E"         VALUE="[2]"/>
            </REGION>
    
            <REGION REF="1">
                <P VAR="rho"       VALUE="[3]"/>
                <P VAR="rhou"      VALUE="[3]"/>
                <P VAR="rhov"      VALUE="[3]"/>
                <P VAR="E"         VALUE="[3]"/>
            </REGION>
    
            <REGION REF="2">
                <P VAR="rho"       VALUE="[0]"/>
                <P VAR="rhou"      VALUE="[0]"/>
                <P VAR="rhov"      VALUE="[0]"/>
                <P VAR="E"         VALUE="[0]"/>
            </REGION>
    
            <REGION REF="3">
                <P VAR="rho"       VALUE="[1]"/>
                <P VAR="rhou"      VALUE="[1]"/>
                <P VAR="rhov"      VALUE="[1]"/>
                <P VAR="E"         VALUE="[1]"/>
            </REGION>
        </BOUNDARYCONDITIONS>

    </CONDITIONS>

</NEKTAR>

