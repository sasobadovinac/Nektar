<?xml version="1.0" encoding="utf-8"?>
<NEKTAR>  
    <GEOMETRY DIM="2" SPACE="2">
        <VERTEX COMPRESSED="B64Z-LittleEndian" BITSIZE="64"></VERTEX>
        <EDGE COMPRESSED="B64Z-LittleEndian" BITSIZE="64"></EDGE>
        <ELEMENT>
            <T COMPRESSED="B64Z-LittleEndian" BITSIZE="64"></T>
            <Q COMPRESSED="B64Z-LittleEndian" BITSIZE="64">eJxdzstSAQAARuGeoZm2jaGiREoog6TSTRcRueWSpHJ5/52Fszr/5lv9Mye0sV4O81jAHXmBRbzUbxdLeIXX+u3hDZbxVr+weu7wHrcwop4HfNRvXz0VfNLvQD3P+KJfVD2vWMUAHqrnDWv6xdRTx3f94uppYFO/I/V8YAu3MaGeNnb0O1ZPF3v6najnE/v6JdUzwCFu4ql6RvilX0o9Y/zWL62eCf7ol1HPFH8xiGfq+cN//c7VM8O5fln1LHCp3wqstSL2</Q>
        </ELEMENT>
        <COMPOSITE>
            <C ID="1"> T[0-34] </C>
            <C ID="2"> Q[35-58] </C>
            <C ID="3"> T[59-93] </C>
            <C ID="100"> E[46,42,0,43-44] </C>
            <C ID="200"> E[50,24,60,63,66,69,139,155] </C>
            <C ID="300"> E[153,148,114,149,151] </C>
            <C ID="400"> E[48,27,105,107,109,111,136,157] </C>
        </COMPOSITE>
        <DOMAIN> C[1-3] </DOMAIN>
    </GEOMETRY>
    <EXPANSIONS>     
        <E COMPOSITE="C[1]" NUMMODES="6" TYPE="MODIFIED" FIELDS="u" />
        <E COMPOSITE="C[2]" NUMMODES="6" TYPE="MODIFIED" FIELDS="u" />
        <E COMPOSITE="C[3]" NUMMODES="6" TYPE="MODIFIED" FIELDS="u" />
    </EXPANSIONS>
    <CONDITIONS>
        <SOLVERINFO>
            <I PROPERTY="EQTYPE"                VALUE="UnsteadyDiffusion" /> 
	        <I PROPERTY="Projection"            VALUE="Continuous"        />
	        <I PROPERTY="TimeIntegrationMethod" VALUE="BackwardEuler"     />
	        <I PROPERTY="DiffusionAdvancement"  VALUE="Implicit"          />
	        <I PROPERTY="Driver"                VALUE="Parareal"          />
        </SOLVERINFO>

	    <PARAMETERS>
            <P> TimeStep        = 0.0001 </P> <!-- Time step -->
	        <P> NumSteps        = 3200   </P> <!-- Number of time step -->
            <P> epsilon         = 0.05   </P> <!-- Diffusion coefficent   -->
            <P> IO_InfoSteps    = 3200   </P> <!-- Frequency of printing information for the user -->
            <P> IO_CheckSteps   = 3200   </P> <!-- Frequency of writing the checkpoints, the results -->
            <P> PararealIterMax = 2      </P> <!-- Max number of Parareal iteration -->
	    </PARAMETERS>

        <!-- setting the variable -->
        <VARIABLES>
            <V ID="0"> u </V>
        </VARIABLES>

        <!-- Defining boundary regions -->
        <BOUNDARYREGIONS>
            <B ID="0"> C[100,200,300,400] </B>
        </BOUNDARYREGIONS>

        <BOUNDARYCONDITIONS>
            <REGION REF="0">
                <D VAR="u" VALUE="0" />
            </REGION>
        </BOUNDARYCONDITIONS>

        <FUNCTION NAME="InitialConditions">
            <E VAR="u"  VALUE="sin(2*PI*x)*sin(2*PI*y)" />
        </FUNCTION>

        <FUNCTION NAME="ExactSolution">
            <E VAR="u"  VALUE="exp(-epsilon*8*PI*PI*t)*sin(2*PI*x)*sin(2*PI*y)" />
        </FUNCTION>
    </CONDITIONS>
</NEKTAR>
