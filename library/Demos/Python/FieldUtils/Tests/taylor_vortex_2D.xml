<?xml version="1.0" encoding="utf-8"?>

<NEKTAR xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.nektar.info/nektar.xsd">

    <GEOMETRY DIM="2" SPACE="2">
        <VERTEX COMPRESSED="B64Z-LittleEndian" BITSIZE="64"></VERTEX>
        <EDGE COMPRESSED="B64Z-LittleEndian" BITSIZE="64"></EDGE>
        <ELEMENT>
            <Q COMPRESSED="B64Z-LittleEndian" BITSIZE="64"></Q>
        </ELEMENT>
        <COMPOSITE>
            <C ID="0"> Q[0-63] </C>
            <C ID="1"> E[0,25,42,59,76,93,110,127] </C>
            <C ID="2"> E[128,130,132,134,136,138,140,142] </C>
            <C ID="3"> E[23,41,58,75,92,109,126,143] </C>
            <C ID="4"> E[3,6,9,12,15,18,21,24] </C>
        </COMPOSITE>
        <DOMAIN> C[0] </DOMAIN>
    </GEOMETRY>
    <Metadata>
        <Provenance>
            <GitBranch>refs/heads/feature/parareal-mpi</GitBranch>
            <GitSHA1>8e18cf72205b25952a30a4638a30894474cb0867</GitSHA1>
            <Hostname>erc-hpc-comp014</Hostname>
            <NektarVersion>5.2.0</NektarVersion>
            <Timestamp>11-Oct-2022 19:16:20</Timestamp>
        </Provenance>
        <NekMeshCommandLine>square-quads.msh square-quads-8x8.xml </NekMeshCommandLine>
    </Metadata>

    <EXPANSIONS>
        <E COMPOSITE="C[0]" NUMMODES="8" FIELDS="u,v,p" TYPE="MODIFIED"          />
    </EXPANSIONS>

    <CONDITIONS>

        <SOLVERINFO>
            <I PROPERTY="SolverType"            VALUE="VelocityCorrectionScheme" />
            <I PROPERTY="EQTYPE"                VALUE="UnsteadyNavierStokes"     />
            <I PROPERTY="AdvectionForm"         VALUE="Convective"               />
            <I PROPERTY="Projection"            VALUE="Galerkin"                 />
            <I PROPERTY="TimeIntegrationMethod" VALUE="IMEXOrder1"               />
        </SOLVERINFO>

        <PARAMETERS>
            <P> TimeStep      = 0.0004   </P>
            <P> NumSteps      = 16000    </P>
            <P> IO_CheckSteps = 16000    </P>
            <P> IO_InfoSteps  = 16000    </P>
            <P> Re            = 100      </P>
            <P> Kinvis        = 1/Re     </P>
        </PARAMETERS>

        <VARIABLES>
            <V ID="0"> u </V>
            <V ID="1"> v </V>
            <V ID="2"> p </V>
        </VARIABLES>

        <BOUNDARYREGIONS>
            <B ID="0"> C[1] </B>
            <B ID="1"> C[2] </B>
            <B ID="2"> C[3] </B>
            <B ID="3"> C[4] </B>
        </BOUNDARYREGIONS>

        <BOUNDARYCONDITIONS>
            <REGION REF="0">
                <D VAR="u" USERDEFINEDTYPE="TimeDependent" VALUE="-cos(x)*sin(y)*exp(-2*t*Kinvis)" />
                <D VAR="v" USERDEFINEDTYPE="TimeDependent" VALUE="sin(x)*cos(y)*exp(-2*t*Kinvis)" />
                <N VAR="p" USERDEFINEDTYPE="H" VALUE="0.0" />
            </REGION>
            <REGION REF="1">
                <D VAR="u" USERDEFINEDTYPE="TimeDependent" VALUE="-cos(x)*sin(y)*exp(-2*t*Kinvis)" />
                <D VAR="v" USERDEFINEDTYPE="TimeDependent" VALUE="sin(x)*cos(y)*exp(-2*t*Kinvis)" />
                <D VAR="p" USERDEFINEDTYPE="TimeDependent" VALUE="-0.25*(cos(2*x)+cos(2*y))*exp(-4*t*Kinvis)" />
            </REGION>
            <REGION REF="2">
                <D VAR="u" USERDEFINEDTYPE="TimeDependent" VALUE="-cos(x)*sin(y)*exp(-2*t*Kinvis)" />
                <D VAR="v" USERDEFINEDTYPE="TimeDependent" VALUE="sin(x)*cos(y)*exp(-2*t*Kinvis)" />
                <N VAR="p" USERDEFINEDTYPE="H" VALUE="0.0" />
            </REGION>
            <REGION REF="3">
                <D VAR="u" USERDEFINEDTYPE="TimeDependent" VALUE="-cos(x)*sin(y)*exp(-2*t*Kinvis)" />
                <D VAR="v" USERDEFINEDTYPE="TimeDependent" VALUE="sin(x)*cos(y)*exp(-2*t*Kinvis)" />
                <D VAR="p" USERDEFINEDTYPE="TimeDependent" VALUE="-0.25*(cos(2*x)+cos(2*y))*exp(-4*t*Kinvis)" />
            </REGION>
        </BOUNDARYCONDITIONS>

        <FUNCTION NAME="InitialConditions">
            <E VAR="u" VALUE="-cos(x)*sin(y)" />
            <E VAR="v" VALUE="sin(x)*cos(y)" />
            <E VAR="p" VALUE="-0.25*(cos(2*x)+cos(2*y))" />
        </FUNCTION>

        <FUNCTION NAME="ExactSolution">
            <E VAR="u" VALUE="-cos(x)*sin(y)*exp(-2*Kinvis*t)" />
            <E VAR="v" VALUE="sin(x)*cos(y)*exp(-2*Kinvis*t)" />
            <E VAR="p" VALUE="-0.25*(cos(2*x)+cos(2*y))*exp(-4*Kinvis*t)" />
        </FUNCTION>

    </CONDITIONS>

</NEKTAR>
