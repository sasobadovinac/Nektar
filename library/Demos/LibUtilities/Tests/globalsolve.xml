<NEKTAR>

<GLOBALSOLVE>
  <SOLVECONFIG>
    <XxtConfig Name="UWConfig" >
      <Type> MultiLevelStaticCond </Type>
    </XxtConfig>
      
    <XxtConfig Name="VConfig" >
      <Type> StaticCond </Type> <!-- can be Full, MultiLevel, StaticCond -->
    </XxtConfig>
      
    <IterativeConfig Name="PConfig" >
      <Type> StaticCond </Type>
      <Method> ConjugateGradient </Method>
      <Precon>  LowEnergyBlocK </Precon>
      <AbsTol>  1e-8 </AbsTol>
      <MaxIter> 5000 </MaxIter> 
    </IterativeConfig>
  </SOLVECONFIG>
  
  <SOLVE>
    <VelocitySolve>
      <ScalarSolve Var="u,w"> UWConfig </ScalarSolve>
      <ScalarSolve Var="v">   VConfig  </ScalarSolve>
    </VelocitySolve>
      
    <PressureSolve>
      <ScalarSolve Var="p"> PConfig  </ScalarSolve>
    </PressureSolve>
  </SOLVE>
  
  <!--- JFNK for global solve !-->
  <PRECONCONFIG>
    <BlockJacobiConfig Name="BlockDiagConfig" >
      <NumIter> 3  </NumIter>
    </BlockJacobiConfig>
  </PRECONCONFIG>
  
  <SOLVECONFIG>
      <!--- JF Newton solver !-->
      <NewtonConfig Name="JFNKConfig">
        <JacEval>    JacFree  </JacEval>
        <LinSolver>  GMRes    </LinSolver>
        <AbsTol>     1e-6     </AbsTol>
      </NewtonConfig>
      

      <IterativeConfig Name="GMRes">
        <Method> GMRes </Method>
        <Precon>  BlockDiagConfig </Precon>
        <AbsTol>  1e-6  </AbsTol>
        <MaxIter> 5000 </MaxIter>
      </IterativeConfig>
  </SOLVECONFIG>

  <SOLVE>
    <CFSImplicitSolve>
      <VectorSolve Var="rho,rhou,rhov,rhow,rhoE"> JFNKConfig </VectorSolve>
    </CFSImplicitSolve>
  </SOLVE>
</GLOBALSOLVE>

</NEKTAR>
